{
    "name": "taktix-api",
    "version": "1.0.0",
    "description": "taktix api for mobile and admin web",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --exec babel-node src/index.js",
        "clean": "rm -rf ./build && mkdir build",
        "prebuild": "npm run clean",
        "build": "babel src -d ./build --ignore ./node_modules",
        "db:setup": "sequelize db:create",
        "db:migrate:up": "sequelize db:migrate",
        "db:migrate:down": "sequelize db:migrate:undo:all",
        "db:refresh": "npm run db:migrate:down && npm run db:migrate:up",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "author": "Faqqih,Kevin",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
        "cloudinary": "^1.32.0",
        "cors": "^2.8.5",
        "datauri": "^4.1.0",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "express-validator": "^6.14.2",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^2.3.3",
        "nanoid": "^3.3.4",
        "nodemailer": "^6.7.8",
        "nodemon": "^2.0.20",
        "passport": "^0.6.0",
        "passport-google-id-token": "^0.4.7",
        "passport-google-token": "^0.1.2",
        "randomstring": "^1.2.2",
        "redis": "^4.6.6",
        "sequelize": "^6.23.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.4",
        "@babel/core": "^7.15.5",
        "@babel/node": "^7.15.4",
        "@babel/plugin-proposal-optional-chaining": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.4",
        "@babel/register": "^7.15.3",
        "@babel/runtime": "^7.15.4",
        "babel-eslint": "^10.1.0",
        "babel-node": "0.0.1-security",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "nodemon": "^2.0.12",
        "prettier": "^2.7.1",
        "sequelize-cli": "^6.4.1"
    }
}
